
{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": 0,
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": ["error", {"allow":["_id"]}]
  }
}